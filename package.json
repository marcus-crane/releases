  "bugs": {
    "url": "https://github.com/vgdates/backend/issues"
  },
  "homepage": "https://github.com/vgdates/backend#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "got": "^6.7.1",
    "knex": "^0.13.0",
    "moment": "^2.18.1",
    "pg": "^6.1.5"
  },
  "devDependencies": {
    "standard": "^8.6.0"
  }
}
